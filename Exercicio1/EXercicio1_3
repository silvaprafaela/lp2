public class Exercicio1_3{

	public static void main(String [] args){
 
	int a = 45;
 	int b = 20;
 	int c = 3;
	System.out.println ( " O resultado da soma é: " + (a+b+c));
	System.out.println ( " O resultado da multiplicação é: " + (a*b*c));
	System.out.println ( " O resultado da divisão é: " + (a/b/c));
	System.out.println ( " O resultado da subtração é: " + (a-b-c));

		}
}
